<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="registerContainer" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.forum.RegisterController">
   <Label layoutX="191.0" layoutY="14.0" text="Register Page" textFill="WHITE">
      <font>
         <Font name="System Bold" size="35.0" />
      </font>
   </Label>
   <GridPane layoutX="37.0" layoutY="103.0" prefHeight="219.0" prefWidth="533.0">
      <columnConstraints>
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="156.0" minWidth="10.0" prefWidth="156.0" />
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="173.0" minWidth="0.0" prefWidth="22.0" />
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="355.0" minWidth="10.0" prefWidth="355.0" />
      </columnConstraints>
      <rowConstraints>
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
      <Label text="Username: " textFill="WHITE">
         <font>
            <Font name="System Bold" size="27.0" />
         </font>
      </Label>
      <Label text="Password:" textFill="WHITE" GridPane.rowIndex="1">
         <font>
            <Font name="System Bold" size="27.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" GridPane.columnIndex="2">
         <font>
            <Font name="System Italic" size="27.0" />
         </font></TextField>
      <PasswordField fx:id="passwordField" GridPane.columnIndex="2" GridPane.rowIndex="1">
         <font>
            <Font name="System Italic" size="27.0" />
         </font>
      </PasswordField>
   </GridPane>
   <Label fx:id="status" alignment="CENTER" layoutX="7.0" layoutY="65.0" prefHeight="39.0" prefWidth="590.0" textAlignment="CENTER" textFill="#ff0404">
      <font>
         <Font name="System Bold Italic" size="27.0" />
      </font>
   </Label>
   <Button fx:id="returnBtn" layoutX="37.0" layoutY="328.0" mnemonicParsing="false" onMouseClicked="#logInPage" text="Return">
      <font>
         <Font size="18.0" />
      </font>
   </Button>
   <Button fx:id="registerBtn" layoutX="481.0" layoutY="328.0" mnemonicParsing="false" onMouseClicked="#register" text="Register">
      <font>
         <Font size="18.0" />
      </font>
   </Button>
</AnchorPane>
