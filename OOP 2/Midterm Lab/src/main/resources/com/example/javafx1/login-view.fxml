<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="logInContainer" id="logInContainer" alignment="CENTER" prefHeight="265.0" prefWidth="469.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx1.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
  <children>
  <GridPane id="container" prefHeight="170.0" prefWidth="364.0">
    <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="143.0" minWidth="10.0" prefWidth="143.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="138.0" minWidth="0.0" prefWidth="0.0" />
         <ColumnConstraints hgrow="SOMETIMES" maxWidth="286.0" minWidth="10.0" prefWidth="286.0" />
    </columnConstraints>
    <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <Label prefHeight="40.0" prefWidth="224.0" text="WELCOME">
      <font>
        <Font size="28.0" />
      </font>
    </Label>
    <Label text="Username: " GridPane.rowIndex="1">
      <font>
        <Font size="21.0" />
      </font>
    </Label>
    <TextField id="usernameField" fx:id="usernameField" GridPane.columnIndex="2" GridPane.rowIndex="1">
      <font>
        <Font size="16.0" />
      </font>
    </TextField>
    <Label text="Password: " GridPane.rowIndex="2">
      <font>
        <Font size="21.0" />
      </font>
    </Label>
    <TextField id="passwordField" fx:id="passwordField" GridPane.columnIndex="2" GridPane.rowIndex="2">
      <font>
        <Font size="16.0" />
      </font>
    </TextField>
      <Label fx:id="status" prefHeight="35.0" prefWidth="279.0" textFill="#ff1111" GridPane.columnIndex="2">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
  </GridPane>
  <Button onAction="#onHelloButtonClick" onKeyPressed="#onHelloButtonClick" onMouseClicked="#onHelloButtonClick" text="Sign in" fx:id="logOutButton" id="logOutButton" />
  </children>
</VBox>
