<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="mainPageContainer" prefHeight="301.0" prefWidth="479.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx1.LogInGUI.HelloController">
    <GridPane layoutX="6.0" layoutY="23.0" prefHeight="263.0" prefWidth="560.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="485.0" minWidth="10.0" prefWidth="485.0"/>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="274.0" minWidth="10.0" prefWidth="75.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Label text="Welcome">
            <font>
                <Font name="System Bold Italic" size="31.0"/>
            </font>
        </Label>
        <Button fx:id="logOutButton" mnemonicParsing="false" onMouseClicked="#logOutClick"
                text="Log Out" GridPane.rowIndex="2"/>
        <ColorPicker fx:id="colorPicker" GridPane.rowIndex="1"/>
    </GridPane>
</AnchorPane>
