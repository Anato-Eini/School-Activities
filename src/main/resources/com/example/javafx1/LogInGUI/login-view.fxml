<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="logInContainer" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx1.LogInGUI.LogInController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
  <GridPane id="container" layoutX="37.0" layoutY="103.0" prefHeight="219.0" prefWidth="533.0">
    <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="175.0" minWidth="10.0" prefWidth="175.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="138.0" minWidth="0.0" prefWidth="15.0" />
      <ColumnConstraints hgrow="SOMETIMES" maxWidth="354.0" minWidth="10.0" prefWidth="354.0" />
    </columnConstraints>
    <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <Label prefHeight="39.0" prefWidth="167.0" text="Username: ">
      <font>
        <Font name="System Bold" size="27.0" />
      </font>
    </Label>
    <TextField id="usernameField" fx:id="usernameField" GridPane.columnIndex="2">
      <font>
        <Font name="System Bold" size="27.0" />
      </font>
    </TextField>
    <Label prefHeight="50.0" prefWidth="355.0" text="Password: " GridPane.rowIndex="1">
      <font>
        <Font name="System Bold" size="27.0" />
      </font>
    </Label>
      <PasswordField fx:id="passwordField" GridPane.columnIndex="2" GridPane.rowIndex="1">
         <font>
            <Font name="System Italic" size="27.0" />
         </font>
      </PasswordField>
  </GridPane>
  <Button id="" fx:id="logInButton" layoutX="491.0" layoutY="322.0" onAction="#onHelloButtonClick" onKeyPressed="#onHelloButtonClick" onMouseClicked="#onHelloButtonClick" text="Sign in">
      <font>
         <Font size="18.0" />
      </font></Button>
 <Label layoutX="207.0" layoutY="14.0" text="Log-in Page" textAlignment="CENTER">
   <font>
     <Font name="System Bold" size="35.0" />
   </font>
 </Label>
   <Button fx:id="registerBtn" layoutX="37.0" layoutY="322.0" mnemonicParsing="false" onMouseClicked="#registerPage" text="Register">
      <font>
         <Font size="18.0" />
      </font></Button>
   <Label fx:id="status" layoutX="58.0" layoutY="73.0" prefHeight="39.0" prefWidth="495.0" textFill="RED">
      <font>
         <Font name="System Bold Italic" size="27.0" />
      </font></Label>
</AnchorPane>
