<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="mainPageContainer" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.forum.MainPageController">
   <Button fx:id="logOutButton" layoutX="14.0" layoutY="364.0" mnemonicParsing="false" onMouseClicked="#logOutClick" text="Log Out" />
